{
    "name": "larabra/laravel-media-library-input",
    "description": "HTML input for Spatie Media Library",
    "keywords": [
        "laravel", 
        "spatie", 
        "laravel-medialibrary", 
        "media",
        "images",
        "downloads"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ennio Sousa",
            "email": "ennio@enniosousa.com.br",
            "homepage": "https://github.com/enniosousa"
        }
    ],
    "homepage": "https://github.com/larabra/laravel-media-library-input",
    "require": {
        "illuminate/support": "~7|~8",
        "laravelcollective/html": "^6.2",
        "spatie/laravel-medialibrary": "^9.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Larabra\\LaravelMediaLibraryInput\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larabra\\LaravelMediaLibraryInput\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larabra\\LaravelMediaLibraryInput\\LaravelMediaLibraryInputServiceProvider"
            ]
        }
    }
}
